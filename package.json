{
  "name": "internetarchive-ts",
  "version": "0.1.0",
  "description": "Archive.org API wrapper written in Typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "cross-env NODE_ENV=testing ts-mocha --timeout 1000 \"./src/**/*.spec.ts\"",
    "test:coverage": "nyc --reporter=html --reporter=text --reporter=text-summary npm test",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && tsc",
    "doc": "typedoc && cp ./internet-archive-ts-logo.svg ./doc"
  },
  "keywords": [
    "archive.org",
    "internet",
    "archive",
    "api"
  ],
  "author": "Thomas Novotny",
  "license": "ISC",
  "dependencies": {
    "minimatch": "^9.0.3",
    "rfc6902": "^5.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^24.10.1",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "nyc": "^15.1.0",
    "ts-mocha": "^10.0.0",
    "typedoc": "^0.27.6"
  }
}
